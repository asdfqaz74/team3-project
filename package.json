{
  "scripts": {
    "dev": "run-p serve watch",
    "sass": "sass --load-path=node_modules cafe/src/scss:cafe/src/stylesheets",
    "serve": "five-server",
    "watch": "npm run sass -- --watch",
    "clean": "rimraf cafe/src/stylesheets",
    "build": "npm run sass -- --no-source-map --style compressed"
  },
  "dependencies": {
    "five-server": "^0.3.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.5"
  }
}
